{
  "manifest_version": 2,
  "name": "Ecosia Search Icons",
  "version": "1.0.2",
  "description": "Displays the icon for search results on Ecosia",
  "content_scripts": [
    {
      "matches": ["*://*.ecosia.org/*"],
      "js": ["addIcons.js"]
    }
  ],
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "storage",
    "https://icons.duckduckgo.com/ip3/*"
    ],
    "icons":{
      "128": "ESI.png"
    }
}